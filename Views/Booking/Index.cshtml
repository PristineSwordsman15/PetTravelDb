@model IEnumerable<PetTravelDb.Models.BookingProcess>
<div style="background-color: mediumturquoise; backface-visibility: 0%">

@{
    ViewData["Title"] = "Bookings";
}

<h1>Bookings</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BookingProcessID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BookingProcessID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BookingProcessID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
